{
  "name": "@kyc/root",
  "version": "1.0.0",
  "description": "Monorepo para o sistema KYC de validação facial",
  "private": true,
  "scripts": {
    "dev:api": "pnpm --filter @kyc/api dev",
    "dev:web": "pnpm --filter @kyc/web dev",
    "dev": "pnpm -r --parallel dev",
    "build:api": "pnpm --filter @kyc/api build",
    "build:web": "pnpm --filter @kyc/web build",
    "build": "pnpm -r build",
    "start:api": "pnpm --filter @kyc/api start",
    "start:web": "pnpm --filter @kyc/web start",
    "download-models": "pnpm --filter @kyc/api download-models",
    "test": "pnpm -r test"
  },
  "keywords": [
    "face",
    "recognition",
    "kyc",
    "validation",
    "monorepo"
  ],
  "author": "Darlan Jr. <kelgaxd@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.5",
    "concurrently": "^9.1.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.8.0"
}
